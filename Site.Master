<style>
.loader 
{
 position: fixed;
 left: 0px;
 top: 0px;
 width: 100%;
 height: 100%;
 z-index: 9999;
 background: url('<%= Page.ResolveUrl("~/images/loader2.gif")%>') 50% 50% no-repeat rgb(249,249,249);
}
</style>
		<script type="text/javascript">
		function promptmev2(promptmessage)
		{
		alert(promptmessage);
		}		
		function selecttab(tabname)
		{
                   $('#tabselect').val(tabname);
                   alert($('#tabselect').val());
		}
		function promptme(promptmessage,navpageto)
		{
		var bar = new $.peekABar({
		cssClass: 'promptmessage',
		delay: 2000,
		backgroundColor: '#0288d1',
		autohide: true,
		position: 'bottom',
		onHide: function() {
         location.href=navpageto;
         }
		});
        bar.show({
           html: promptmessage
        });		
		}
		function alertme(promptmessage)
		{
		var bar = new $.peekABar({
		cssClass: 'promptalert',
		delay: 2000,
		backgroundColor: 'red',
		autohide: true,
		position: 'bottom'
		});
        bar.show({
           html: promptmessage
        });		
		}
		function informme(promptmessage)
		{
		var bar = new $.peekABar({
		cssClass: 'promptmessage',
		delay: 2000,
		backgroundColor: '#1ba1e2',
		autohide: true,
		position: 'bottom'
		});
        bar.show({
           html: promptmessage
        });		
		}	
		 </script>

